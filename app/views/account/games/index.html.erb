<h1>Account::Games#index</h1>
<p>Find me in app/views/account/games/index.html.erb</p>
<p>User:</p>
<% if current_user.photo.attachment %>
  <%= cl_image_tag current_user.photo.key, height: 300, width: 400, crop: :fill %>
<% end %>
<p>---------------------</p>
<% @games.each do |game| %>
<h3><%= game.name %></h3>
<%= link_to "edit", edit_account_game_path(game)  %>
<%= link_to "delete", account_game_path(game), method: :delete %>
<% end %>
